@model List<MealBox.Models.BankModel>

@{
    ViewBag.Title = "BankList";
}


<h2>Bank List</h2>

@section Styles {
    <link href="~/plugins/datatables-autofill/css/jquery.dataTables.css" rel="stylesheet" />

}

<div class="card-header">
    <a style="float:right;" type="button" class="btn btn-success" href="@Url.Action("AddBank","Managment")">Add Bank</a>
</div>


<table id="mytable" class="table-bordered ">
    <thead>
        <tr>
            <th style="text-align:center"></th>
            <th style="text-align:center">Bank Name</th>

            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td></td>
                <td>@item.CashBnk_nam</td>
                <td><a style="width:100%; height:100%" class="btn btn-success" href="@Url.Action("AddBank","Managment", new { id = @item.CashBnk_id})"><span>Edit</span></a></td>
            </tr>
        }
    </tbody>
</table>

@section Scripts
    {

    <script src="~/plugins/datatables-autofill/js/jquery.dataTables.js"></script>
    <script type="text/javascript">


        $(document).ready(function () {
            $('#mytable').DataTable();
        });


    </script>

}
