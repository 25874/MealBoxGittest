<script type="text/javascript">

    $(document).ready(function () {
        var total = 0;
        $('#Exptbl tbody .select-row2 ').each(function (index, ele) {
            total += parseFloat($(this).find('td:eq(2)').text());
        })
        $('#lblttl').text(total);
    })

</script>
